<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/launchscreen"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:elevation="40dp"
    android:background="@drawable/image_1"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="424dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="117dp">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="42dp"
            android:layout_height="36dp"
            android:elevation="51dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.654"
            android:src="@drawable/ellipse_16"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/ellipse_15"
            android:layout_width="345dp"
            android:layout_height="365dp"
            android:background="@drawable/ellipse_15"
            android:elevation="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:contentDescription="TODO" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:elevation="51dp"
            app:layout_constraintBottom_toBottomOf="@+id/ellipse_15"
            app:layout_constraintEnd_toEndOf="@+id/ellipse_15"
            app:layout_constraintStart_toStartOf="@+id/ellipse_15"
            app:layout_constraintTop_toTopOf="@+id/ellipse_15"
            app:layout_constraintVertical_bias="0.238"
            android:src="@drawable/cooking_2" />

        <TextView
            android:id="@+id/title"
            android:layout_width="182dp"
            android:layout_height="48dp"
            android:elevation="51dp"
            android:text="WSR Food"
            android:textColor="#000000"
            android:textSize="48sp"
            app:fontFamily="@font/italianno"
            app:layout_constraintBottom_toTopOf="@+id/imageView13"
            app:layout_constraintEnd_toEndOf="@+id/ellipse_15"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/ellipse_15"
            app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</RelativeLayout>